        "@netlify/functions": "^1.3.0",
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/line-clamp": "^0.4.2",
        "@tailwindcss/typography": "^0.5.8",
        "@types/busboy": "^1.5.0",
        "@types/lunr": "^2.3.4",
        "@types/node": "^18.11.18",
        "@types/quill": "^2.0.9",
        "@types/sharp": "^0.31.0",
        "@typescript-eslint/parser": "^5.43.0",
        "astro": "2.0.0-beta.2",
        "astro-icon": "^0.8.0",
        "astro-remote": "^0.2.3",
        "astro-webfinger": "^1.2.0",
        "clsx": "^1.2.1",
        "date-fns": "^2.29.3",
        "dotenv": "^16.0.3",
        "eslint": "^8.27.0",
        "eslint-plugin-astro": "^0.21.0",
